{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noEmit": false,
    "emitDeclarationOnly": false,
    "outDir": "dist",
    "declaration": true
  },
  "exclude": ["__tests__"]
}
