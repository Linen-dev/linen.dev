{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "emitDeclarationOnly": true,
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "declaration": true,
    "paths": {
      "services/*": ["../../apps/web/services/*"],
      "utilities/*": ["../../apps/web/utilities/*"],
      "queue/*": ["../../apps/web/queue/*"],
      "config": ["../../apps/web/config"],
      "config/*": ["../../apps/web/config/*"],
      "mailers/*": ["../../apps/web/mailers/*"],
      "@linen/types": ["../types/src"]
    }
  }
}
