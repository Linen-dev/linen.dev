{
  "name": "@linen/ui",
  "private": true,
  "version": "1.0.0",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "watch": "yarn babel --watch src --out-dir dist --extensions '.ts,.tsx,.js,.jsx'",
    "prebuild": "rm -rf dist && yarn tsc",
