    "postbuild": "yarn tsc --project tsconfig.types.json"
  },
  "dependencies": {
    "react": "^18.2.0",
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/react": "^18.0.25",
    "babel-plugin-transform-scss": "^1.1.0",
    "sass": "^1.56.1",
    "sass-loader": "^13.2.0",
    "typescript": "^4.9.3"
  }
}
