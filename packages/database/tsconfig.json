{
  "compilerOptions": {
    "target": "es5",
    "moduleResolution": "node",
    "allowJs": false,
    "noEmit": false,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "declaration": true,
    "declarationDir": "dist",
    "outDir": "dist"
  },
  "include": ["src/index.ts"],
  "exclude": ["node_modules", "../../node_modules/*"]
}
