{
  "compilerOptions": {
    "target": "ES6",
    "moduleResolution": "node",
    "allowJs": true,
    "noEmit": false,
    "strict": true,
    "esModuleInterop": true,
    "declaration": true,
    "declarationDir": "dist",
    "outDir": "dist",
    "skipLibCheck": true
  },
  "include": ["src"],
  "exclude": ["src/*.test.ts"]
}
