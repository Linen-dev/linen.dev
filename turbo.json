{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "outputs": [
        "dist/**",
        ".next/**",
        "public/dist/**"
      ],
      "dependsOn": [
        "^build"
      ]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    },
    "test": {
      "cache": false,
      "dependsOn": [
        "^test"
      ]
    },
    "start": {
      "outputs": [
        ".next/**"
      ]
    }
  }
}