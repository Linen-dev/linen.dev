{
  "name": "community",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "cd .. && npx -y husky install",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "setup:integration": "dotenv -e .env.test -- npm run db:migrate:test",
    "test:integration": "dotenv -e .env.test -- jest -i",
    "integration": "dotenv -e .env.test -- jest -c jest.config.integration.js",
    "actualIntegration": "dotenv -e .env.test -- jest -c jest.config.actualIntegration.js",
    "db:migrate": "npx prisma generate && npx prisma migrate dev",
    "db:migrate:test": "dotenv -e .env.test -- npx prisma migrate deploy",
    "db:seed": "ts-node --skip-project bin/seed.ts",
    "start:prod": "pm2-runtime --raw \"npm run start\"",
    "script:sync:discord": "ts-node -P tsconfig.commonjs.json bin/discord-sync/index.ts",
    "script:sync:slack": "ts-node -P tsconfig.commonjs.json bin/slack-sync/index.ts",
    "script:remove-community": "ts-node -P tsconfig.commonjs.json bin/remove-community/index.ts",
    "script:maintenance": "ts-node -P tsconfig.commonjs.json bin/maintenance/index.ts",
    "start:queue:webhook": "pm2-runtime --raw \"npm run queue:worker:webhook\"",
    "queue:worker:webhook": "dotenv -e .env -- npx ts-node -P tsconfig.commonjs.json queue/workers/webhook.ts",
    "queue:worker:webhookRunOnce": "npm run queue:worker:webhook -- --once"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.2",
    "@fortawesome/free-brands-svg-icons": "^6.1.2",
    "@fortawesome/free-solid-svg-icons": "^6.1.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@headlessui/react": "^1.6.6",
    "@prisma/client": "^3.15.2",
    "@sentry/nextjs": "^7.11.1",
    "@types/debug": "^4.1.7",
    "@types/marked": "^4.0.5",
    "@types/micro": "^7.3.7",
    "@types/node": "17.0.45",
    "@types/nodemailer": "^6.4.5",
    "@types/nprogress": "^0.2.0",
    "@types/react": "17.0.48",
    "@types/stripe": "^8.0.417",
    "@types/superagent": "^4.1.15",
    "@types/uuid": "^8.3.4",
    "autoprefixer": "^10.4.8",
    "autosize": "^5.0.1",
    "awesome-debounce-promise": "^2.1.0",
    "aws-sdk": "^2.1197.0",
    "axios": "^0.27.2",
    "classnames": "^2.3.1",
    "debug": "^4.3.4",
    "entities": "^4.3.1",
    "graphile-worker": "^0.13.0",
    "highlight.js": "^11.6.0",
    "marked": "^4.0.18",
    "micro": "^9.4.1",
    "next": "^12.2.5",
    "next-auth": "^4.10.3",
    "next-s3-upload": "^0.2.3",
    "next-use-posthog": "^1.16.0",
    "node-cache": "^5.1.2",
    "nodemailer": "^6.7.8",
    "nprogress": "^0.2.0",
    "pm2": "^5.2.0",
    "prisma": "^3.15.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-emoji-render": "^1.2.4",
    "react-error-boundary": "^3.1.4",
    "react-hot-toast": "^2.3.0",
    "react-icons": "^4.4.0",
    "react-infinite-scroll-hook": "^4.0.3",
    "react-intersection-observer": "^8.34.0",
    "react-paginate": "^8.1.3",
    "react-twitter-embed": "^4.0.4",
    "sitemap": "^7.1.1",
    "stripe": "^8.222.0",
    "superagent": "^7.1.5",
    "swr": "^1.3.0",
    "tailwindcss": "^3.1.8",
    "timeago.js": "^4.0.2",
    "typescript": "^4.7.4",
    "eslint": "8.22.0",
    "uuid": "^8.3.2",
    "eslint-config-next": "^12.2.5"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.2.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.4.3",
    "aws-cdk-lib": "^2.38.0",
    "dotenv-cli": "^5.1.0",
    "esbuild": "^0.15.5",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-mock-extended": "^2.0.7",
    "lint-staged": "^12.5.0",
    "next-test-api-route-handler": "^3.1.7",
    "postcss": "^8.4.16",
    "prettier": "2.7.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
